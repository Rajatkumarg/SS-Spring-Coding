<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.rajat.beans4.Customer" id="custId1">
		<property name="name" value="Rajat" />
		<property name="acc">
			<bean class="com.rajat.beans4.Account" id="accId">
				<property name="acctId" value="acc101"></property>
				<property name="balance" value="1000"></property>
			</bean>
		</property>
	</bean>

	<bean class="com.rajat.beans4.Customer" id="custId2">
		<property name="name" value="Nishant" />
		<property name="acc">
			<bean class="com.rajat.beans4.Account" id="accId">
				<property name="acctId" value="acc102"></property>
				<property name="balance" value="2000"></property>
			</bean>
		</property>
	</bean>

	<bean class="com.rajat.beans4.Student" id="stdObj1">
		<property name="name" value="Nishant" />
		<property name="rollno" value="1"></property>
		<property name="subjects">
			<array>
				<value>Physics</value>
				<value>Chemistry</value>
				<value>Maths</value>
			</array>
		</property>
	</bean>


	<bean class="com.rajat.beans4.Employee" id="id3">
		<property name="empId" value="103" />
		<property name="empName" value="Chetan" />
	</bean>
	<bean class="com.rajat.beans4.Company" id="compId1">
		<property name="compName" value="Infosys" />
		<property name="workers">
			<array>
				<bean class="com.rajat.beans4.Employee">
					<property name="empId" value="101" />
					<property name="empName" value="Rajat" />
				</bean>
				<bean class="com.rajat.beans4.Employee">
					<property name="empId" value="102" />
					<property name="empName" value="Nishant" />
				</bean>
				<ref bean="id3" />
			</array>
		</property>
	</bean>


	<bean class="com.rajat.beans4.Company" id="compObj2">
		<property name="compName" value="Infosys" />
		<property name="workers">
			<array>
				<bean class="com.rajat.beans4.Employee">
					<property name="empId" value="101" />
					<property name="empName" value="Rajat" />
				</bean>
				<bean class="com.rajat.beans4.Employee">
					<property name="empId" value="102" />
					<property name="empName" value="Nishant" />
				</bean>
				<ref bean="id3" />
			</array>
		</property>
		<property name="workerList">
			<list>
				<bean class="com.rajat.beans4.Employee">
					<property name="empId" value="101" />
					<property name="empName" value="Rajat" />
				</bean>
				<bean class="com.rajat.beans4.Employee">
					<property name="empId" value="102" />
					<property name="empName" value="Nishant" />
				</bean>
				<ref bean="id3" />
			</list>
		</property>
	</bean>


	<bean class="com.rajat.beans4.Student" id="stdObj2">
		<property name="name" value="Nishant" />
		<property name="rollno" value="1"></property>
		<property name="subjects">
			<array>
				<value>Physics</value>
				<value>Chemistry</value>
				<value>Maths</value>
			</array>
		</property>
		<property name="subjectList">
			<list>
				<value>Phy</value>
				<value>Chem</value>
				<value>Maths</value>
			</list>
		</property>
		<property name="marks">
			<list>
				<value>98</value>
				<value>99</value>
				<value>100</value>
			</list>
		</property>
		<property name="mobileNo">
			<bean
				class="org.springframework.beans.factory.config.ListFactoryBean">
				<property name="targetListClass">
					<value>java.util.LinkedList</value>
				</property>
				<property name="sourceList">
					<list>
						<value>5453453454</value>
						<value>5465466454</value>
						<value>9885457543</value>
					</list>
				</property>
			</bean>
		</property>
		<property name="emails">
			<set>
				<value>def@gmail.com</value>
				<value>abc@gmail.com</value>
				<value>ghi@gmail.com</value>
			</set>
		</property>
		<property name="emialsInIncOrder">
			<bean
				class="org.springframework.beans.factory.config.SetFactoryBean">
				<property name="targetSetClass">
					<value>java.util.TreeSet</value>
				</property>
				<property name="sourceSet">
					<list>
						<value>def@gmail.com</value>
						<value>abc@gmail.com</value>
						<value>ghi@gmail.com</value>
					</list>
				</property>
			</bean>
		</property>
		<property name="subjectMarksMap">
			<map>
				<entry key="JSE" value="98" />
				<entry key="C" value="95" />
				<entry key="JEE" value="91" />
				<entry key="C++" value="99" />
				<entry key="Spring" value="100" />
			</map>
		</property>
		<property name="subjectMarksMapAsc">
			<bean
				class="org.springframework.beans.factory.config.MapFactoryBean">
				<property name="targetMapClass">
					<value>java.util.TreeMap</value>
				</property>
				<property name="sourceMap">
					<map>
						<entry key="JSE" value="98" />
						<entry key="C" value="95" />
						<entry key="JEE" value="91" />
						<entry key="C++" value="99" />
						<entry key="Spring" value="100" />
					</map>
				</property>
			</bean>
		</property>
	</bean>


</beans>
